; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ArabicTyping"
run/main_scene="res://scenes/loading_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/low_processor_mode=true
run/delta_smoothing=false
boot_splash/bg_color=Color(0.980392, 0.964706, 0.952941, 1)
boot_splash/image="res://assets/white_grid.png"

[autoload]

LearningSystemsManager="*res://scripts/autoload/LearningSystemsManager.gd"
UserSettingsManager="*res://scripts/autoload/UserSettingsManager.gd"
VocabularyManager="*res://scripts/autoload/VocabularyManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true
